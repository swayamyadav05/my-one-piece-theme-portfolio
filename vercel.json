{
    "version": 2,
    "functions": {
        "api/chat.js": {
            "runtime": "@vercel/node@18"
        }
    },
    "builds": [
        {
            "src": "api/chat.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/chat",
            "dest": "/api/chat.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}